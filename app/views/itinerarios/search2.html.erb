<% #Provide the page title in a symbol to application.html.erb to avoid duplication %>
<% provide :title, 'Risultati ricerca itinerari' %>
<%= render 'layouts/header' %>


<a href=<%= homepage_users_path %>>
  <div class="img_logo">
    <%= image_tag("Viaggi del Guizaglio_Logo7.png") %>
  </div>
</a>

<div id="tot_colonnasx_home">
  <canvas id="canvas_img" width="500" height="600">Non supportato</canvas>

  <script type="text/javascript">
      var canvas = document.getElementById('canvas_img');
      var ctx = canvas.getContext('2d');

      var img= new Image();
      img.src = "/assets/maddie-spiaggia.jpg";
      img.onload = function(){

          ctx.drawImage(img,0,0);
      }
  </script>
</div>

<div id="tot_colonnadx_home">



  <div class="span8">
    <h2> Risultati della ricerca per la citta di <%= params[:search2] %>: </h2>
    <% @itinerarios = Itinerario.search2(params[:search2]).paginate(page: params[:page], :per_page => 10)  %>

    <% if @itinerarios.any? %>

        <ul class="users">
          <%= render @itinerarios %>
        </ul>
        <%= will_paginate @itinerarios %>
    <% end %>
  </div>
</div>


</div>




