<!-- Vista parziale nel carousel della homepage : gli ultimi tre itinerari -->
<ol class="itinerari">
    <% @itinerarios=Itinerario.all.paginate(page: params[:page], :per_page => 3)  %>
    <% @itinerarios.each do |itinerario|  %>
               <%= render itinerario %>
        <h3>Itinerario di <%= link_to User.find(itinerario.user_id).name, User.find(itinerario.user_id)  %></h3>

<% end %>
</ol>



